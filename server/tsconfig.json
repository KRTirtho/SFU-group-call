{
  "compilerOptions": {
    "incremental": true,
    "target": "es5",
    "lib": [
      "ESNext"
    ],
    "module": "commonjs",
    "outDir": "./dist",
    "noEmit": false,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true /* Skip type checking all .d.ts files. */
  },
  "include": [
    "."
  ]
}